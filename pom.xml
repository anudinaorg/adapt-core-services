		<springLdapVersion>2.0.4.RELEASE</springLdapVersion>
		<slf4JVersion>1.7.12</slf4JVersion>
		<logbackVersion>1.1.3</logbackVersion>
		<tomcatVersion>7.0.42</tomcatVersion>
		<wss4jVersion>1.6.16</wss4jVersion>
